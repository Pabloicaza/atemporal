<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<div class="container" style="margin-top:30px;">
  <table class="table">
    <thead>
      <tr>
        <th>Titulo de proyecto</th>
        <th>Tipo de proyecto</th>
        <th>Ubicaci√≥n</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <% @proyectos.each do |proyecto| %>
      <tr>
        <td><%= proyecto.nombre_proyecto %></td>
        <td><%= proyecto.tipo_proyecto %></td>
        <td><%= proyecto.ubicacion %></td>
        <td><%= link_to 'Ver', proyecto_path(proyecto) %></td>
        <td><%= link_to 'Editar', edit_proyecto_path(proyecto) %></td>
        <td><%= link_to 'Borrar', proyecto_path(proyecto),
                method: :delete%></td>
      </tr>
    <% end %>
  </table>
</div>
